generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Project {
  id          Int          @id @default(autoincrement())
  name        String       @unique
  deliverables Deliverable[]
}

model Deliverable {
  id          Int      @id @default(autoincrement())
  projectId   Int
  project     Project  @relation(fields: [projectId], references: [id])
  title       String
  dueDate     DateTime
  frequency   String?
  manager     String
}
